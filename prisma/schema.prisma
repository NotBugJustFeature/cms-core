datasource db1 {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

// generated fields

model User {
  id Int @id @default(autoincrement())
  name String? 
  email String? 
  password String? 
  posts Post?
}

model Post {
  id Int @id @default(autoincrement())
  title String @unique @default("1") 
  content String? 
  ownerId Int @unique
  owner User @relation(fields: [ownerId], references: [id])
}

