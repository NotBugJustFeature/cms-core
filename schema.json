{
    "info": {
        "version": "1.0.0",
        "title": "Prisma Cms Core",
        "description": "core plugin",
        "slug": "prisma-cms-core",
        "type": "core"
    },
    "collections": {
        "User": {
            "fields": {
                "name": {
                    "type": "Text"
                },
                "email": {
                    "type": "Text"
                },
                "password": {
                    "type": "Text"
                }
            },
            "info": {
                "defaultField": "name"
            }
        },
        "Post": {
            "fields": {
                "title": {
                    "type": "Text",
                    "required": true,
                    "unique": true
                },
                "content": {
                    "type": "Text"
                }
            },
            "info": {
                "defaultField": "name"
            }
        }
    },
    "relations": [
        {
            "source_entity": "User",
            "source_field": "posts",
            "target_entity": "Post",
            "target_field": "owner",
            "type": "one-to-many"
        }
    ]
}
